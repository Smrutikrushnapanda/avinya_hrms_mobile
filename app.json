{
  "expo": {
    "name": "Avinya",
    "slug": "Avinya",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/App-logo.png",
    "scheme": "hrms",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/images/App-logo.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "Work Reminders"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/App-logo.png",
      "bundleIdentifier": "com.smrutipanda.HRMS"
    },
    "android": {
      "icon": "./assets/images/App-logo.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/App-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "useNextNotificationsApi": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.smrutipanda.HRMS"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/App-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/App-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take check-in photos."
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location to validate attendance."
        }
      ],
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      "expo-notifications",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "22863903-72c8-4846-b0e0-a24950513dcd"
      }
    },
    "owner": "smrutikrushnapanda"
  }
}
